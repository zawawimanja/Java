public class Main {

	public static void main(String[] args) {   			int [][]a = new int[10][10];    

					for(int i = 0; i < 10; i++)

							{

										for(int j = 0; j < i; j++)

													{

																	System.out.print(""+a[i][j]);

																				}

																							System.out.println();

																									}

																										}

																										}

